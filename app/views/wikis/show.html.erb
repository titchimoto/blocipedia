<h1><%= @wiki.title %></h1>

<p><%= markdown(@wiki.body) %></p>

<p>Created by: <%= @wiki.user.email %></p>

<% if user_signed_in? %>
  <%= link_to "Edit Wiki", edit_wiki_path(@wiki) %>
  <%= link_to "Delete Wiki", @wiki, method: :delete, data: {confirm: 'Are you sure you want to delete this post?'} %>
<% end %>
